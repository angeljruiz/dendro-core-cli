{
  "name": "dendro-cli",
  "version": "0.0.11",
  "description": "Core CLI for dendroflumechuck",
  "keywords": [
    "oclif",
    "time-series",
    "AWS",
    "serverless",
    "logs"
  ],
  "homepage": "https://github.com/dendro-naap/dendro-cli",
  "bugs": "https://github.com/dendro-naap/dendro-cli/issues",
  "repository": "dendro-naap/dendro-cli",
  "license": "MIT",
  "author": "team-naap",
  "main": "lib/index.js",
  "bin": {
    "dendro": "./bin/run"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/lib",
    ".next/server",
    ".next/static",
    ".next/BUILD_ID",
    ".next/build-manifest.json",
    ".next/export-marker.json",
    ".next/images-manifest.json",
    ".next/prerender-manifest.json",
    ".next/react-loadable-manifest.json",
    ".next/required-server-files.json",
    ".next/routes-manifest.json"
  ],
  "scripts": {
    "dev": "node ./src/server.js",
    "build:configs": "node lib/vector/index.js",
    "build:next": "next build",
    "build:go": "GOOS=linux CGO_ENABLED=0 go build -o ./src/aws/lambda/go/main ./src/aws/lambda/go/*.go",
    "build:oclif": "oclif-dev manifest && oclif-dev readme",
    "build:tsc": "tsc --noEmit false",
    "build:move_zip": "mv ./src/aws/lambda/go/function.zip ./lib/aws/lambda/function.zip",
    "prepack": "npm run build:oclif && npm run build:next && npm run build:tsc && npm run zip:go && npm run build:move_zip",
    "postpack": "rm -f oclif.manifest.json",
    "start": "NODE_ENV=production node ./src/server.js",
    "version": "oclif-dev readme && git add README.md",
    "zip": "zip -j ./src/aws/lambda/go/function.zip ./src/aws/lambda/go/main",
    "zip:go": "npm run build:go && npm run zip"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "aws-sdk": "^2.884.0",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "conf": "^9.0.2",
    "enquirer": "^2.3.6",
    "js-file-download": "^0.4.12",
    "next": "10.1.3",
    "ora": "^5.4.0",
    "prompts": "^2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "^1.14.1",
    "victory": "^35.5.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/adm-zip": "^0.4.34",
    "@types/node": "^10.17.56",
    "@types/prompts": "^2.0.10",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "globby": "^10.0.2",
    "postcss": "^8.2.10",
    "tailwindcss": "^2.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dendro",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
